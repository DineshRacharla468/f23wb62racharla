extends layout

block content
  h1= title
 
  - let output = [{Name :"apricotdelight",Calories :"124cal",Price:100 },{Name :"laddu",Calories :"87cal",Price:15 },{Name :"badhamsweet",Calories :"113cal",Price:45}];
 
  div.container-fluid
    div.row
        each Desserts, index in output
            div.col-sm-2
                p Product Name:  #{Desserts.Name}
                p Brand: #{Desserts.Calories}
                p Price: #{Desserts.Price}